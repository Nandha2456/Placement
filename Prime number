import java.util.*;
class prime
{
    public static void main(String[] s){
        Scanner number=new Scanner(System.in);
        System.out.println("Enter a Number : ");
        int x=number.nextInt();
        if (x<=1){
            System.out.println("Not prime Number");
        }
        for(int i=2;i<=x;i++){
        if (x%i==0){
            System.out.println("Not prime Number");
        }else{
            System.out.println("prime Number");
        }
        break;
      }
    }
}
